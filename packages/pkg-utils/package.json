{
  "name": "@monorepo-setup/pkg-utils",
  "type": "module",
  "version": "0.0.1",
  "description": "comprehensive utility library for monorepo demonstration - includes TypeScript types, async utilities, validation, caching, event system, and performance monitoring",
  "author": "sutie <susususutie@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/susususutie/monorepo-setup/tree/main/packages/pkg-utils#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/susususutie/monorepo-setup.git"
  },
  "bugs": "https://github.com/susususutie/monorepo-setup/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.mjs",
      "require": "./dist/constants.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./math": {
      "types": "./dist/math.d.ts",
      "import": "./dist/math.mjs",
      "require": "./dist/math.cjs"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "import": "./dist/string.mjs",
      "require": "./dist/string.cjs"
    },
    "./array": {
      "types": "./dist/array.d.ts",
      "import": "./dist/array.mjs",
      "require": "./dist/array.cjs"
    },
    "./object": {
      "types": "./dist/object.d.ts",
      "import": "./dist/object.mjs",
      "require": "./dist/object.cjs"
    },
    "./async": {
      "types": "./dist/async.d.ts",
      "import": "./dist/async.mjs",
      "require": "./dist/async.cjs"
    },
    "./date": {
      "types": "./dist/date.d.ts",
      "import": "./dist/date.mjs",
      "require": "./dist/date.cjs"
    },
    "./validator": {
      "types": "./dist/validator.d.ts",
      "import": "./dist/validator.mjs",
      "require": "./dist/validator.cjs"
    },
    "./event": {
      "types": "./dist/event.d.ts",
      "import": "./dist/event.mjs",
      "require": "./dist/event.cjs"
    },
    "./cache": {
      "types": "./dist/cache.d.ts",
      "import": "./dist/cache.mjs",
      "require": "./dist/cache.cjs"
    },
    "./error": {
      "types": "./dist/error.d.ts",
      "import": "./dist/error.mjs",
      "require": "./dist/error.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "unbuild",
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "unbuild",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage"
  },
  "devDependencies": {
    "unbuild": "catalog:",
    "vitest": "catalog:",
    "@vitest/coverage-v8": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  }
}
