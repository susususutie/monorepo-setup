{
  "name": "@monorepo-setup/pkg-vue-ui",
  "type": "module",
  "version": "0.1.0",
  "description": "no-bundler Vue UI components library",
  "author": "sutie <susususutie@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/susususutie/monorepo-setup/tree/main/packages/pkg-vue-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/susususutie/monorepo-setup.git"
  },
  "bugs": "https://github.com/susususutie/monorepo-setup/issues",
  "keywords": [
    "vue",
    "ui-components",
    "component-library"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/button/index.d.ts",
      "import": "./dist/button/index.js"
    },
    "./card": {
      "types": "./dist/card/index.d.ts",
      "import": "./dist/card/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "vue": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "vue-tsc": "catalog:"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && pnpm copy-vue-files",
    "copy-vue-files": "cp -r src/components/*.vue dist/components/ 2>/dev/null || true"
  },
  "publishConfig": {
    "access": "public"
  }
}